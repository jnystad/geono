    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.46.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.64.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6"
  }
